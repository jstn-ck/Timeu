{
  "name": "Timeu",
  "version": "1.0.0",
  "description": "A simple timetracking application",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jstn-ck/timeu"
  },
  "homepage": "",
